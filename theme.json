{
  "name": "Bootstrap Superadmin",
  "version": "0.0.1",
  "homepage": "https://github.com/JumpLink/cms",
  "authors": [
    "Pascal Garber <pascal@jumplink.eu>"
  ],
  "description": "Default Superadmin of JumpLink CMS",
  "image": "assets/images/preview.png",
  "modernview": "views/modern/init.jade",
  "license": "MIT",
  "setup": {
    "users": [
      {
        "email": "admin@admin.org",
        "name": "admin",
        "color": "#000000",
        "password": "cms-admin",
        "role": "superadmin"
      }
    ],
    "routes": [
      {
        "position": 1,
        "match": "/*home*",
        "title": "Home",
        "state": {
          "name": "layout.home",
          "customstate": false,
          "url": "",
          "views": "",
          "resolve": "",
          "parent": "Layout"
        },
        "fallback": {
          "url": ""
        },
        "site": "admin",
        "main": true,
        "key": "home",
        "objectName": "layoutHome",
        "sitetitle": "JumpLink CMS Administration - Home",
        "url": "/home",
        "navbar": "header"
      },
      {
        "position": 2,
        "match": "/*themes*",
        "title": "Themes",
        "state": {
          "name": "layout.themes",
          "customstate": false,
          "url": "",
          "views": "",
          "resolve": "",
          "parent": "Layout"
        },
        "fallback": {
          "url": ""
        },
        "site": "admin",
        "main": true,
        "key": "themes",
        "objectName": "layoutThemes",
        "navbar": "header",
        "sitetitle": "JumpLink CMS Administration - Themes",
        "url": "/themes"
      },
      {
        "position": 3,
        "match": "/*routes*",
        "title": "Routes",
        "state": {
          "name": "layout.routes",
          "customstate": false,
          "url": "",
          "views": "",
          "resolve": "",
          "parent": "Layout"
        },
        "fallback": {
          "url": ""
        },
        "site": "admin",
        "main": true,
        "key": "routes",
        "objectName": "layoutRoutes",
        "sitetitle": "JumpLink CMS Administration - Routes",
        "navbar": "header",
        "url": "/routes"
      },
      {
        "position": 4,
        "match": "",
        "title": "Users",
        "main": true,
        "state": {
          "name": "layout.users",
          "customstate": false,
          "url": "",
          "views": "",
          "resolve": "",
          "parent": "layout"
        },
        "url": "/users",
        "alternativeUrls": [],
        "fallback": {
          "url": ""
        },
        "objectName": "layoutUsers",
        "key": "users",
        "sitetitle": "JumpLink CMS Administration - Users",
        "navbar": "header",
        "site": "admin"
      }
    ]
  }
